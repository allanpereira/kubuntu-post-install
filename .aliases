alias g=git
alias k=kubectl
alias c=clear
alias cls=clear
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias gk='gitk --all &'
alias up='git pull origin $(git rev-parse --abbrev-ref HEAD)'
alias pbcopy='xsel — clipboard — input'
alias pbpaste='xsel — clipboard — output'
alias clip='xclip -o -selection clipboard'
alias repos='cd $HOME/repos'
